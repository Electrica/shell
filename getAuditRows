#!/bin/sh

#FOO=( '133723018\*143' '133967677\*139' '133968458\*144' '133968469\*134' '133988879\*148' '133989246\*138' '133989729\*143' '134006402\*147' '134006604\*137' '134006832\*142' '134029543\*135' '134030150\*145' '134030159\*140' )
FOO=( '133664885\*138' '133723018\*143' '133897654\*145' '133967677\*139' '133968458\*144' '133968469\*134' '133988879\*148' '133989729\*143' '134006402\*147' '134006604\*137' '134006832\*142' '134029543\*135' '134030159\*140' )
for ((i=0;i<${#FOO[@]};i++)); do
   echo ${i} ${FOO[${i}]}
   grep -m 60 "${FOO[${i}]}" $1 | tail -$2 >> auditfile$2.csv
done
